<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wta.NewCloudApp.jiuwei210278">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name="com.wta.NewCloudApp.config.App"
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme_NOACTIONBAR">
        <provider
            android:authorities="${applicationId}.provider"
            android:name="com.wta.NewCloudApp.config.FileProVider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths"/>
        </provider>
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="66e8559c8e8a3ec03f6bb1e028721bcc" />
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="5b1ddaf8f29d9850ae0000e1" />
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="unknown" /> <!-- 跟推广渠道有关系，暂时写unknown -->
        <meta-data
            android:name="com.wta.NewCloudApp.config.GlobalConfiguration"
            android:value="ConfigModule" />

        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.MainActivity"
            android:exported="true"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity android:name="com.wta.NewCloudApp.mvp.ui.activity.BaseListActivity" />
        <activity
            android:theme="@style/AppTheme_FULLSCREEN"
            android:name="com.wta.NewCloudApp.mvp.ui.activity.LoginActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity android:name="com.wta.NewCloudApp.mvp.ui.activity.BaseLoadingActivity" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.WebViewActivity"
            android:screenOrientation="portrait" />
        <!-- third_party -->
        <!-- sina -->
        <activity
            android:name="com.umeng.socialize.media.WBShareCallBackActivity"
            android:configChanges="keyboardHidden|orientation"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.sina.weibo.sdk.web.WeiboSdkWebActivity"
            android:configChanges="keyboardHidden|orientation"
            android:exported="false"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.sina.weibo.sdk.share.WbShareTransActivity"
            android:launchMode="singleTask"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="com.sina.weibo.sdk.action.ACTION_SDK_REQ_ACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!-- wx -->
        <activity
            android:name=".wxapi.WXEntryActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:exported="true"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <!-- qq -->
        <activity
            android:name="com.tencent.tauth.AuthActivity"
            android:launchMode="singleTask"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="tencent100424468" /> <!-- 需要换成自己的 -->
            </intent-filter>
        </activity>
        <activity
            android:name="com.tencent.connect.common.AssistActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.SweepActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.UserMsgActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.UserQRActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.BServiceActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.NameSetActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.ShopInActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme_FULLSCREEN">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.GuideActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme_FULLSCREEN" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.SettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.AuthActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.MsgActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.BindPhoneActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.GroupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.AddRecActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wta.NewCloudApp.mvp.ui.activity.CardListActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.wta.NewCloudApp.mvp.ui.activity.AddCardActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>